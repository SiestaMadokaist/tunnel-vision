{
	"name": "ramadoka.com",
	"version": "0.1.0",
	"type": "commonjs",
	"engines": {
		"node": "16"
	},
	"private": true,
	"scripts": {
		".prepare": "husky install",
		"pre-commit-lint": "lint-staged",
		"sls:local": "TS_NODE_PROJECT='tsconfig.sls.json' node --inspect -r ts-node/register src/index.ts",
		"test": "node -r ts-node/register src/crons/pandadoc-debug.ts",
		"sls:compile": "tsc --skipLibCheck -p tsconfig.sls.json",
		"sls:deploy": "npm run sls:compile && serverless deploy",
		"ui:deploy": "rm -rf out && npm run build && npm run export && aws s3 cp ./out s3://ramadoka.com --acl public-read --recursive",
		"dev": "next dev",
		"build": "next build",
		"export": "next export",
		"start": "next start",
		"lint": "next lint"
	},
	"dependencies": {
		"@aws-sdk/client-dynamodb": "^3.67.0",
		"@vendia/serverless-express": "^4.5.4",
		"axios": "^0.27.2",
		"body-parser": "^1.20.0",
		"cors": "^2.8.5",
		"dotenv": "^16.0.0",
		"dynamoose": "^3.0.0-beta.1",
		"express": "^4.17.3",
		"next": "12.1.4",
		"react": "18.0.0",
		"react-dom": "18.0.0"
	},
	"devDependencies": {
		"@serverless/typescript": "^3.0.0",
		"@types/aws-lambda": "^8.10.71",
		"@types/body-parser": "^1.19.2",
		"@types/cors": "^2.8.12",
		"@types/express": "^4.17.14",
		"@types/node": "^14.14.25",
		"esbuild": "^0.14.11",
		"json-schema-to-ts": "^1.5.0",
		"serverless": "^3.0.0",
		"serverless-esbuild": "^1.23.3",
		"ts-node": "^10.4.0",
		"tsconfig-paths": "^3.9.0",
		"typescript": "^4.6.3"
	},
	"lint-staged": {
		"*.{ts,tsx}": [
			"prettier",
			"eslint"
		]
	}
}
