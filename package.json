{
	"name": "tunnel-vision",
	"version": "0.1.6",
	"type": "commonjs",
	"engines": {
		"node": "16"
	},
	"private": false,
	"bin": "./tunnelvision.js",
	"scripts": {
		".prepare": "husky install",
		"pre-commit-lint": "lint-staged",
		"compile": "tsc",
		"prepublish": "npm run compile",
		"local": "node -r ts-node/register src/functions/local/app.ts",
		"remote": "node -r ts-node/register src/functions/remote/app.ts",
		"package": "serverless package --verbose",
		"sls": "serverless",
		"deploy": "npm run compile && serverless deploy",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"dependencies": {
		"@aws-sdk/client-sqs": "^3.171.0",
		"axios": "^0.27.2",
		"commander": "^9.4.0",
		"dotenv": "^16.0.2",
		"sqs-consumer": "^5.7.0"
	},
	"devDependencies": {
		"@types/node": "^14.14.25",
		"aws-sdk": "^2.1222.0",
		"ts-node": "^10.4.0",
		"tsconfig-paths": "^3.9.0",
		"typescript": "^4.6.3"
	},
	"lint-staged": {
		"*.{ts,tsx}": [
			"prettier",
			"eslint"
		]
	}
}
